{
  "name": "composable-machine",
  "version": "0.0.0",
  "description": "Wrapper for xstate to allow machine composition",
  "main": "ComposableMachine.ts",
  "scripts": {
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "release": "echo 'Incrementing version and publishing to npmjs.com...' && cp package.json ./build/package.json && np",
    "test": "jest",
    "version": "echo 'Executing npm publish hook: version... (rebuilds ./build)' && npm run build"
  },
  "keywords": [
    "Eluvio",
    "xstate"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/eluv-io/composable-machine"
  },
  "sideEffects": false,
  "author": "Michael Parker",
  "license": "MIT",
  "dependencies": {
    "@eluvio/elv-js-helpers": "^6.1.0",
    "xstate": "^4.37.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.6",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.4.3",
    "np": "^7.6.3",
    "rimraf": "^4.3.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
